{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.3.0",
    "asvae/laravel-api-tester": "^2.0",
    "fideloper/proxy": "~4.0",
    "fruitcake/laravel-cors": "^2.0",
    "guzzlehttp/guzzle": "^7.3",
    "jwilsson/spotify-web-api-php": "^4.3",
    "ksubileau/color-thief-php": "^1.4",
    "laravel/framework": "8.75.0",
    "laravel/helpers": "^1.2",
    "laravel/sanctum": "^2.8",
    "laravel/tinker": "^2.0",
    "laravel/ui": "^3.0",
    "league/fractal": "^0.19.2",
    "madcoda/php-youtube-api": "^1.2",
    "rap2hpoutre/laravel-log-viewer": "^1.6",
    "symfony/process": "^5.2"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.1",
    "filp/whoops": "~2.0",
    "fakerphp/faker": "^1.14.0",
    "mockery/mockery": "1.4.2",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "files": [
      "app/Global/utils.php",
      "app/Global/media.php"
    ],
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "test": [
      "vendor/phpunit/phpunit/phpunit --colors --testdox"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
